<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="toolTitleBottom">
        <item name="android:textSize">40sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/grayToolTitleBottom</item>
    </style>

    <style name="text">
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/grayText</item>
    </style>

    <style name="buttom">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorLink">@color/grayToolTitleBottom</item>
        <item name="android:textColorHighlight">@color/grayToolTitleBottom</item>
    </style>

    <style name="header">
        <item name="android:textSize">25sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/grayToolTitleBottom</item>
    </style>

    <style name="spinner">
        <item name="android:background">@color/spinner</item>
        <item name="android:dropDownSelector">@color/black</item>
        <item name="android:drawable">@drawable/ic_seta_spinner</item>
    </style>

    <style name="bottomNav">
        <item name="android:textColor">@color/grayToolTitleBottom</item>
        <item name="itemIconTint">@color/grayToolTitleBottom</item>
        <item name="itemTextColor">@color/grayToolTitleBottom</item>
    </style>

    <style name="toolbar" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="titleTextColor">@color/white</item>
        <item name="android:background">@color/grayToolTitleBottom</item>
    </style>

</resources>

